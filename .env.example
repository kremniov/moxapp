# MoxApp - Environment Configuration
# Copy this file to .env and fill in the values

# Load test configuration
LOAD_MULTIPLIER=1.0
CONCURRENT_REQUESTS=20

# Base URL for example endpoints
EXAMPLE_BASE_URL=https://api.example.com

# Auth examples
EXAMPLE_API_KEY=your-api-key
EXAMPLE_BEARER_TOKEN=your-bearer-token
EXAMPLE_CUSTOM_HEADER=your-custom-token
EXAMPLE_BASIC_USER=your-basic-user
EXAMPLE_BASIC_PASS=your-basic-pass

# Token refresh example
EXAMPLE_TOKEN_URL=https://auth.example.com/oauth/token
EXAMPLE_CLIENT_ID=your-client-id
EXAMPLE_CLIENT_SECRET=your-client-secret